{
  "name": "@agentman/chat-widget",
  "version": "0.12.0",
  "description": "A customizable chat widget for Agentman AI integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "docs",
    "LICENSE"
  ],
  "scripts": {
    "build": "webpack && tsc --emitDeclarationOnly",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "echo \"No lint specified\" && exit 0",
    "dev": "http-server . -p 8000 -c-1",
    "docs": "mkdir -p docs && cp README.md docs/index.md"
  },
  "keywords": [
    "chat",
    "widget",
    "agentman",
    "ai",
    "typescript",
    "react",
    "javascript"
  ],
  "homepage": "https://github.com/Agentman-ai/.github/tree/main/chat-widget-demo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agentman-ai/.github.git",
    "directory": "chat-widget-demo"
  },
  "bugs": {
    "url": "https://github.com/Agentman-ai/.github/issues"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  }
}
